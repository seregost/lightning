{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "allowJs": true,
    "lib": [
      "dom",
      "es7"
    ],
    "baseUrl": ".",
    "paths": {
      "*": [
          "src/types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
